      <MemberSignature Language="F#" Value="member this.AutoCollapseCountThreshold : int with get, set" Usage="Microsoft.Toolkit.Uwp.UI.Controls.BladeView.AutoCollapseCountThreshold" />
      <MemberType>Property</MemberType>
